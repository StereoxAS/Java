
public class ProductEyeBeauty 
{
	private String nameTag;
	private String descTag;
	private int priceTag;
	
	public ProductEyeBeauty(String nameTag, String descTag, String priceTag) 
	{
		// TODO buat constructor-nya 
	}
	public ProductEyeBeauty() 
	{
		//constructor kosong
		this.nameTag = "Belum terisi.";
		this.descTag = "Belum terisi.";
		this.priceTag = 0;
	}
	
	// List Eye make-up buat secukupnya
	// Tiap object (Eclipse/Galaxy) punya setter/getter masing2 juga supaya bisa diakses di class lain
	
	ProductEyeBeauty Eclipse = new ProductEyeBeauty(); //Eyeshadow Pallete 
	public ProductEyeBeauty getEclipse() 
	{
		return Eclipse;
	}
	public void setEclipse(ProductEyeBeauty eclipse) 
	{
		Eclipse = eclipse;
	}

	ProductEyeBeauty Galaxy = new ProductEyeBeauty();  //2-in-1 Glitter Release Eyeliner	
	public ProductEyeBeauty getGalaxy() 
	{
		return Galaxy;
	}
	public void setGalaxy(ProductEyeBeauty galaxy) 
	{
		Galaxy = galaxy;
	}
	// Setter/Getter
	public String getNameTag() 
	{
		return nameTag;
	}
	public void setNameTag(String nameTag) 
	{
		this.nameTag = nameTag;
	}
	public String getDescTag() 
	{
		return descTag;
	}
	public void setDescTag(String descTag) 
	{
		this.descTag = descTag;
	}
	public int getPriceTag() 
	{
		return priceTag;
	}
	public void setPriceTag(int priceTag)
	{
		this.priceTag = priceTag;
	}	
}
