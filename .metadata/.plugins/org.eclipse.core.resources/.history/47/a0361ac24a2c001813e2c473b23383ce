
public class ProductFaceBeauty 
{
	private String nameTag;
	private String descTag;
	private int priceTag;
	
	public ProductFaceBeauty(String nameTag, String descTag, int priceTag) 
	{
		this.nameTag = nameTag;
		this.descTag = descTag;
		this.priceTag = priceTag;
	}
	
	public ProductFaceBeauty() 
	{
		this.nameTag = "Belum terisi.";
		this.descTag = "Belum terisi.";
		this.priceTag = 0;
	}
	
	ProductFaceBeauty Foundation = new ProductFaceBeauty();
	ProductFaceBeauty Primer = new ProductFaceBeauty();
	ProductFaceBeauty Highlighter = new ProductFaceBeauty();
	ProductFaceBeauty Brush = new ProductFaceBeauty();
	// Setter getter objek
	public ProductFaceBeauty getFoundation() {
		return Foundation;
	}

	public void setFoundation(ProductFaceBeauty foundation) {
		Foundation = foundation;
	}

	public ProductFaceBeauty getPrimer() {
		return Primer;
	}

	public void setPrimer(ProductFaceBeauty primer) {
		Primer = primer;
	}

	public ProductFaceBeauty getHighlighter() {
		return Highlighter;
	}

	public void setHighlighter(ProductFaceBeauty highlighter) {
		Highlighter = highlighter;
	}

	public ProductFaceBeauty getBrush() {
		return Brush;
	}

	public void setBrush(ProductFaceBeauty brush) {
		Brush = brush;
	}

	
	
	//Setter getter
	public String getNameTag() {
		return nameTag;
	}

	public void setNameTag(String nameTag) {
		this.nameTag = nameTag;
	}

	public String getDescTag() {
		return descTag;
	}

	public void setDescTag(String descTag) {
		this.descTag = descTag;
	}

	public int getPriceTag() {
		return priceTag;
	}

	public void setPriceTag(int priceTag) {
		this.priceTag = priceTag;
	}
}
