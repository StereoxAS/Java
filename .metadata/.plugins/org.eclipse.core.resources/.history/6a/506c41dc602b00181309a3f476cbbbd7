
public class CharaClassDruid extends CharaClass 
{
	public void getClassDetail()
	{
		className = "Druid";
		classDesc = "The Druids serve the cause of nature and neutrality, the wilderness is their community, "
				+ "and they use their special powers to protect it and to preserve balance in the world.\n"
				+ "Druids have +2 bonus to Dexterity and Wisdom.";
	}
}
