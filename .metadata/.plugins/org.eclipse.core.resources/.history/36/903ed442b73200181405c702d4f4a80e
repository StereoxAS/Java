
public class AccountData extends Account
{
	private AccountUser acc[] = new AccountUser[100];
	
	public AccountData()
	{
		this.acc[0] = new AccountUser();
		AccountUser acc1 = (AccountUser) acc[0];
		acc1.setIdName("MrGantengMaut");
		acc1.setUserName("ganteng");
		acc1.setPassword("1234");
	}

	public AccountUser getAccOfIndex(int index)
	{
		return acc[index];
	}

	public void setAccOfIndex(AccountUser acc, int index)
	{
		this.acc[index] = acc;
	}
}
