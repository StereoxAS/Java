
public class CharaClass 
{
	String className;
	String classDesc;
	
	CharaClass(String className, String classDesc, CharaAttributes attributes)
	{
		this.className = className;
		this.classDesc = classDesc;
	}
	CharaClass()
	{
		this.className = "Undefined";
		this.classDesc = "Undefined";
	}
	
	
}
