
public class Dosen extends Mahasiswa 
{
	private String nip;
	private Dosen dosen;
	
	Dosen() 
	{
		this.nip = "Belum ada.";
		// TODO Auto-generated constructor stub
	}
	Dosen(String nip, String namaDepan)
	{
		this.setNamaDepan(namaDepan);
		this.nip = nip;
	}
	public String getNip()
	{
		return this.nip;
	}
	public static Dosen getDosen()
	{
		return this;
	}
}
