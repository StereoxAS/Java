import java.lang.reflect.Constructor;

public class ProductLipBeauty 
{
	private String nameTag;
	private String descTag;
	private int priceTag;
	
	public ProductLipBeauty(String nameTag, String descTag, int priceTag) 
	{
		this.nameTag = nameTag;
		this.descTag = descTag;
		this.priceTag = priceTag;
	}
	public ProductLipBeauty() 
	{
		this.nameTag = "Belum terisi.";
		this.descTag = "Belum terisi.";
		this.priceTag = 0;
	}
	
	ProductLipBeauty LipStick = new ProductLipBeauty();
	ProductLipBeauty LipGloss = new ProductLipBeauty();
}
