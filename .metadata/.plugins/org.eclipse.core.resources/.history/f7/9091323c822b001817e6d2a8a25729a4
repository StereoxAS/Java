
public class Game 
{
	private static String namaGame;
	private static String devGame;
	private static int priceGame;
	private Game game = new Game();
	
	public Game(String namaGame, String devGame, int priceGame) 
	{
		this.namaGame = namaGame;
		this.devGame = devGame;
		this.priceGame = priceGame;
	}
	public Game() 
	{
		this.namaGame = "-";
		this.devGame = "-";
		this.priceGame = 0;
	}
	
	public static void getGameDetail()
	{
		System.out.println("Game		: " + );
		System.out.println("Developer	: ");
		System.out.println("Price		: ");
	}

	public String getNamaGame() 
	{
		return namaGame;
	}

	public void setNamaGame(String namaGame) 
	{
		this.namaGame = namaGame;
	}

	public String getDevGame() 
	{
		return devGame;
	}

	public void setDevGame(String devGame) 
	{
		this.devGame = devGame;
	}

	public int getPriceGame() 
	{
		return priceGame;
	}

	public void setPriceGame(int priceGame) 
	{
		this.priceGame = priceGame;
	}
	
	
}
