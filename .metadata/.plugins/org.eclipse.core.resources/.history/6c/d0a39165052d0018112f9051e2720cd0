
public class CharaHMS 
{
	private int health;
	private int mana;
	private int stamina;
	private int speed;
	private int magicLevel;
	private int miracleLevel;
	
	public CharaHMS(int health, int mana, int stamina, int speed, int magicLevel, int miracleLevel) 
	{
		this.health = health;
		this.mana = mana;
		this.stamina = stamina;
		this.speed = speed;
		this.magicLevel = magicLevel;
		this.miracleLevel = miracleLevel;
	}
	public CharaHMS() 
	{
		this.health = 0;
		this.mana = 0;
		this.stamina = 0;
		this.speed = 0;
		this.magicLevel = 0;
		this.miracleLevel = 0;
	}
	
	
	
}
