
public class AccountData extends Account
{
	private AccountData acc[] = new AccountData[100];
	
	public AccountData()
	{
		this.acc[0].setIdName("MrGantengMaut");
		this.acc[0].setUserName("ganteng");
		this.acc[0].setPassword("1234");
	}

	public AccountData getAccOfIndex()
	{
		return acc[index];
	}

	public void setAccOfIndex(AccountData acc, int index)
	{
		this.acc[index] = acc;
	}
}
